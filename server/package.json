{
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.11.9",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.10.1",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "NODE_ENV=dev nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "migrate": "yarn prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "4.10.1",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "zod": "^3.20.6"
  }
}
